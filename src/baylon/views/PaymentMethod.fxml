            <Button fx:id="btnPaypal" contentDisplay="CENTER" mnemonicParsing="false" onAction="#selectPaypal" prefHeight="194.0" prefWidth="228.0" text="Paypal" HBox.hgrow="ALWAYS">
               <styleClass>
