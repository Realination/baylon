         <children>
            <Button fx:id="btnCash" mnemonicParsing="false" onAction="#selectCash" prefHeight="194.0" prefWidth="228.0" text="Cash" textAlignment="CENTER" HBox.hgrow="ALWAYS">
               <styleClass>
                  <String fx:value="btn-primary" />
